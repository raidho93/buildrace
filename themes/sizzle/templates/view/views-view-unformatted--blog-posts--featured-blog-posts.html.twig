{#
/**
 * @file
 * Template for Views Blog Post: Featured Blog Post.
 */
#}
{% if title %}
  <h3>{{ title }}</h3>
{% endif %}
<div class="row">
  {% for row in rows %}
    {%
    set row_classes = [
    default_row_class ? 'col-md-6 view__row',
    ]
    %}
    <div{{ row.attributes.addClass(row_classes) }}>
      {{ row.content }}
    </div>
  {% endfor %}
</div>
